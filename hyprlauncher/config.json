{
  "debug": {
    "disable_auto_focus": false,
    "enable_logging": false
  },
  "theme": {
    "colors": {
      "border": "#00ffff",
      "item_bg": "#0e0e0e88",
      "item_bg_hover": "#0e0e0eaa",
      "item_bg_selected": "#303030",
      "item_description": "#a0a0a0",
      "item_description_selected": "#a0a0a0",
      "item_name": "#ffffff",
      "item_name_selected": "#ffffff",
      "item_path": "#cccccc",
      "item_path_selected": "#808080",
      "search_bg": "#0f0f0f",
      "search_bg_focused": "#282828",
      "search_caret": "#00ffff",
      "search_text": "#cccccc",
      "window_bg": "#0e0e0e77"
    },
    "corners": {
      "list_item": 8,
      "search": 8,
      "window": 8
    },
    "spacing": {
      "item_margin": 3,
      "item_padding": 5,
      "search_margin": 10,
      "search_padding": 7
    },
    "typography": {
      "item_description_size": 12,
      "item_name_size": 14,
      "item_path_font_family": "FiraCode Nerd Font Mono",
      "item_path_size": 12,
      "search_font_size": 16
    }
  },
  "window": {
    "anchor": "center",
    "border_width": 2,
    "custom_navigate_keys": {
      "delete_word": "h",
      "down": "j",
      "up": "k"
    },
    "height": 500,
    "margin_bottom": 0,
    "margin_left": 0,
    "margin_right": 0,
    "margin_top": 8,
    "max_entries": 50,
    "show_border": true,
    "show_descriptions": false,
    "show_icons": true,
    "show_paths": true,
    "show_search": true,
    "use_gtk_colors": true,
    "width": 400
  }
}